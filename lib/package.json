{
  "name": "evonext",
  "version": "25.11.15",
  "description": "General library for EvoNext application development.",
  "author": "EvoNext Team",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "npm run build -- --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "dash-platform-sdk": "1.3.0-dev.10",
    "pshenmic-dpp": "1.1.0-dev.7"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sansbankdao/evonext-js.git"
  },
  "bugs": {
    "url": "https://github.com/sansbankdao/evonext-js/issues"
  },
  "homepage": "https://github.com/sansbankdao/evonext-js#readme",
  "keywords": [
    "evonext",
    "dash",
    "dpp"
  ]
}
